        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Send Unique ID" enabled="true">
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters">${__UUID}</stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">String startnum = props.getProperty(&quot;startnum&quot;);
int threadNum = Integer.parseInt(startnum)+ctx.getThreadNum()+1;
String threadno = (threadNum &lt; 10) ? &quot;0&quot; + Integer.toString(threadNum) : Integer.toString(threadNum);
vars.put(&quot;threadno&quot;, threadno);
int base = 36000;
int threadId = threadNum + base;
vars.put(&quot;uniqueid&quot;, Integer.toString(threadId));

</stringProp>
        </BeanShellPreProcessor>
